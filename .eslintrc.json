{
  "extends": [
    "next/core-web-vitals",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  }
}